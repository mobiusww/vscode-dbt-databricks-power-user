{
    "name": "vscode-dbt-bigquery-power-user",
    "displayName": "dbt Power User Plus",
    "description": "This extension makes vscode seamlessly work with dbt and Databricks and BigQuery",
    "icon": "media/dbt.png",
    "publisher": "mobiusww",
    "license": "MIT",
    "homepage": "https://github.com/butchland/vscode-dbt-bigquery-power-user",
    "repository": {
        "type": "git",
        "url": "https://github.com/butchland/vscode-dbt-bigquery-power-user.git"
    },
    "bugs": {
        "url": "https://github.com/butchland/vscode-dbt-bigquery-power-user/issues"
    },
    "version": "0.5.23",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "dbt",
        "sql",
        "jinja-sql",
        "bigquery"
    ],
    "activationEvents": [
        "workspaceContains:**/dbt_project.yml"
    ],
    "main": "./dist/extension",
    "contributes": {
        "configuration": [
            {
                "title": "dbt Power User Plus",
                "properties": {
                    "dbt.listModelsDisabled": {
                        "type": "boolean",
                        "default": false,
                        "description": "Disable automatic listing of models when files change."
                    },
                    "dbt.runModelCommandAdditionalParams": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "default": [],
                        "description": "Add additonal params to the dbt run model command."
                    }

                }
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "dbt_view",
                    "title": "DBT Power User",
                    "icon": "./media/dbt_icon.svg",
                    "contextualTitle": "DBT Power User"
                }
            ]
        },
        "views": {
            "dbt_view": [
                {
                    "id": "parent_model_treeview",
                    "name": "Parent Models"
                },
                {
                    "id": "children_model_treeview",
                    "name": "Children Models"
                }
            ]
        },
        "commands": [
            
            {
                "command": "dbtPowerUser.runCurrentModel",
                "title": "dbt Power User: Run current model",
                "icon": {
                    "light": "./media/run-light.svg",
                    "dark": "./media/run-dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.runDbtGenDocs",
                "title": "dbt Power User: generate DBT docs",
                "icon": {
                    "light": "./media/docs_gen.svg",
                    "dark": "./media/docs_gen_dark.svg"
                }
            },   
            {
                "command": "dbtPowerUser.runDbtServeDocs",
                "title": "dbt Power User: view DBT docs",
                "icon": {
                    "light": "./media/documents.svg",
                    "dark": "./media/documents_dark.svg"
                }
            },                      
            {
                "command": "dbtPowerUser.compileCurrentModel",
                "title": "dbt Power User: Compile current model",
                "icon": {
                    "light": "./media/build_light.svg",
                    "dark": "./media/build_dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.runChildrenModels",
                "title": "dbt Power User: Run children models",
                "icon": {
                    "light": "./media/run-light.svg",
                    "dark": "./media/run-dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.runParentModels",
                "title": "dbt Power User: Run parent models",
                "icon": {
                    "light": "./media/run-light.svg",
                    "dark": "./media/run-dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.showRunSQL",
                "title": "dbt Power User: Show run SQL",
                "icon": {
                    "light": "./media/file-code_light.svg",
                    "dark": "./media/file-code_dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.previewSQL",
                "title": "dbt Power User: Preview SQL",
                "icon": {
                    "light": "./media/file-code_light.svg",
                    "dark": "./media/file-code_dark.svg"
                }
            },
            
            {
                "command": "dbtPowerUser.showCompiledSQL",
                "title": "dbt Power User: Show compiled SQL",
                "icon": {
                    "light": "./media/view_compiled.svg",
                    "dark": "./media/view_compiled_dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.getCurrentTableFromCloud",
                "title": "dbt Power User: get current table from cloud",
                "icon": {
                    "light": "./media/cloud.svg",
                    "dark": "./media/cloud_dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.previewCurrentModel",
                "title": "dbt Power User: preview current model using the compiled SQL",
                "icon": {
                    "light": "./media/preview_table.svg",
                    "dark": "./media/preview_table_dark.svg"
                }
            },
            {
                "command": "dbtPowerUser.runSQLAsIs",
                "title": "dbt Power User: run the SQL in the current window",
                "icon": {
                    "light": "./media/run_sql.svg",
                    "dark": "./media/run_sql_dark.svg"
                }
            }        
           
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "dbtPowerUser.compileCurrentModel",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-8"
                },                
                {
                    "command": "dbtPowerUser.previewCurrentModel",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-7"
                },                                                          
                {
                    "command": "dbtPowerUser.showCompiledSQL",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-6"
                },
                {
                    "command": "dbtPowerUser.runSQLAsIs",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-5"
                },                
                {
                    "command": "dbtPowerUser.runCurrentModel",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-4"
                },
                {
                    "command": "dbtPowerUser.getCurrentTableFromCloud",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-3"
                },


                {
                    "command": "dbtPowerUser.runDbtGenDocs",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-2"
                },   
                {
                    "command": "dbtPowerUser.runDbtServeDocs",
                    "when": "resourceLangId == jinja-sql",
                    "group": "navigation@-1"
                }
            ],
            "view/title": [
                {
                    "command": "dbtPowerUser.runChildrenModels",
                    "when": "view == children_model_treeview",
                    "group": "navigation"
                },
                {
                    "command": "dbtPowerUser.runParentModels",
                    "when": "view == parent_model_treeview",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "dbtPowerUser.runChildrenModels",
                    "when": "view == children_model_treeview && viewItem != source",
                    "group": "inline"
                },
                {
                    "command": "dbtPowerUser.runParentModels",
                    "when": "view == parent_model_treeview && viewItem != source",
                    "group": "inline"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "lint:fix": "eslint src --ext ts --fix",
        "deploy-vscode": "vsce publish",
        "deploy-openvsx": "ovsx publish"
    },
    "devDependencies": {
        "@types/csv-stringify": "^3.1.0",
        "@types/easy-table": "^0.0.33",
        "@types/flat": "^5.0.2",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.13.15",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "file-loader": "^6.1.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "ovsx": "^0.3.0",
        "ts-loader": "^8.0.3",
        "typescript": "^3.8.3",
        "vsce": "^1.103.1",
        "vscode-test": "^1.3.0",
        "webpack": "^5.67.1",
        "webpack-cli": "^4.9.1"
    },
    "extensionDependencies": [
        "samuelcolvin.jinjahtml",
        "ms-python.python"
    ],
    "dependencies": {
        "@google-cloud/bigquery": "^5.12.0",
        "csv-stringify": "^6.0.5",
        "dayjs": "^1.8.34",
        "easy-table": "^1.2.0",
        "flat": "^5.0.2",
        "inversify": "^5.0.5",
        "inversify-binding-decorators": "^4.0.0",
        "package": "^1.0.1",
        "reflect-metadata": "^0.1.13",
        "yaml": "^2.0.0-10"
    }
}
